<application>
  <component name="KeyPromoterStatistic">
    <option name="statistics">
      <Statistic Action="Annotate">
        <Item>
          <IdeaActionID>Annotate</IdeaActionID>
          <ShortCut>'Alt+N'</ShortCut>
          <Description>Annotate</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Check Task">
        <Item>
          <IdeaActionID>Edu.Check</IdeaActionID>
          <ShortCut>'Ctrl+Alt+Enter'</ShortCut>
          <Description>Check Task</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Close Others">
        <Item>
          <IdeaActionID>CloseAllEditorsButActive</IdeaActionID>
          <ShortCut>'Alt+O'</ShortCut>
          <Description>Close Others</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Close Project">
        <Item>
          <IdeaActionID>CloseProject</IdeaActionID>
          <ShortCut>'Alt+J'</ShortCut>
          <Description>Close Project</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Delete">
        <Item>
          <ShortCut>'Delete'</ShortCut>
          <Description>Delete</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Next Difference">
        <Item>
          <ShortCut>'F7'</ShortCut>
          <Description>Next Difference</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
      <Statistic Action="Next Task">
        <Item>
          <IdeaActionID>NextTaskAction</IdeaActionID>
          <ShortCut>'Ctrl+Period'</ShortCut>
          <Description>Next Task</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Previous Difference">
        <Item>
          <ShortCut>'Shift+F7'</ShortCut>
          <Description>Previous Difference</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'Alt+1'</ShortCut>
          <Description>Project</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Reset Task File">
        <Item>
          <IdeaActionID>RefreshTaskAction</IdeaActionID>
          <ShortCut>'Ctrl+Shift+X'</ShortCut>
          <Description>Reset Task File</Description>
          <Count>8</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Alt+T'</ShortCut>
          <Description>Settings...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diff">
        <Item>
          <ShortCut>'Ctrl+D'</ShortCut>
          <Description>Show Diff</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Update Project">
        <Item>
          <IdeaActionID>Vcs.UpdateProject</IdeaActionID>
          <ShortCut>'Ctrl+T'</ShortCut>
          <Description>Update Project</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>